    "@types/node": "^22.5.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "jsdom": "^25.0.0",
    "release-it": "^17.4.1",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5",
    "vue": "^3.4.31"
